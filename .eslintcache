cards/src/components/Card/index.js",["59"],"import React, { Component } from 'react';\nimport cl from 'classnames';\nimport s from './Card.module.scss';\nimport { CheckSquareOutlined, DeleteOutlined } from '@ant-design/icons';\n\nclass Card extends React.Component {\n\n    state = {\n        chosen: false,\n        isRemembered: false, \n    }\n\n    handleCardClick = () => {  \n        if (this.state.isRemembered === false ) {\n            this.setState(({ chosen }) => {\n                return {\n                    chosen: !chosen,\n                }\n            })\n        }                    \n            \n        };\n    \n    handleIsRememberedClick = () => {\n        this.setState(({isRemembered}) => {\n            return {\n                isRemembered: !isRemembered,\n            }\n        });      \n    };\n\n    handleDeletedClick = () => {\n        console.log('###: 1 level');\n        this.props.onDeleted();\n    };\n\n    render() {\n        const {eng, rus} = this.props;\n        const {chosen, isRemembered} = this.state;\n        const cardClass = [s.card];\n        if (chosen) {\n            cardClass.push(s.chosen);\n        }\n\n        return(\n\n            <div \n                className={ cl(s.card, { [s.chosen]: chosen},\n                    { [s.isRemembered]: isRemembered},\n                    { [s.chosen]: isRemembered}) }                \n                >\n                <div className={s.icons}>\n                    <CheckSquareOutlined onClick={this.handleIsRememberedClick}/>    \n                </div>\n                \n                <div className={s.cardInner} onClick={this.handleCardClick}>\n                    <div className={s.cardFront}>\n                        { eng }\n                    </div>\n                    <div className={s.cardBack}>\n                        { rus }\n                    </div>\n                </div>\n\n                <div className={s.icons}>\n                    <DeleteOutlined onClick={this.handleDeletedClick} />\n                </div>\n                \n            </div>\n        )\n    }\n}\n\nexport default Card;","/Users/hang-kharkov/Desktop/else/learn/react/flash-cards/src/components/CardList/index.js",[],"/Users/hang-kharkov/Desktop/else/learn/react/flash-cards/src/wordlist/index.js",["60"],"import React from 'react';\n\nexport const wordsList = [\n    {\n        eng: 'between',\n        rus: 'между',\n        id: 1\n    },\n    {\n        eng: 'high',\n        rus: 'высокий',\n        id: 2\n    },\n    {\n        eng: 'really',\n        rus: 'действительно',\n        id: 3\n    },\n    {\n        eng: 'something',\n        rus: 'что-нибудь',\n        id: 4\n    },\n    {\n        eng: 'most',\n        rus: 'большинство',\n        id: 5\n    },\n    {\n        eng: 'another',\n        rus: 'другой',\n        id: 6\n    },\n    {\n        eng: 'much',\n        rus: 'много',\n        id: 7\n    },\n    {\n        eng: 'family',\n        rus: 'семья',\n        id: 8\n    },\n    {\n        eng: 'own',\n        rus: 'личный',\n        id: 9\n    },\n    {\n        eng: 'out',\n        rus: 'из/вне',\n        id: 10\n    },\n    {\n        eng: 'leave',\n        rus: 'покидать',\n        id: 11\n    },\n    {\n        eng: 'see',\n        rus: 'видеть',\n        id: 12\n    },\n];\n\n","/Users/hang-kharkov/Desktop/else/learn/react/flash-cards/src/components/Section/index.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":2,"column":8,"nodeType":"67","messageId":"68","endLine":2,"endColumn":12},{"ruleId":"61","replacedBy":"69"},{"ruleId":"63","replacedBy":"70"},{"ruleId":"71","severity":1,"message":"72","line":13,"column":5,"nodeType":"73","endLine":13,"endColumn":56},{"ruleId":"61","replacedBy":"74"},{"ruleId":"63","replacedBy":"75"},{"ruleId":"65","severity":1,"message":"76","line":1,"column":17,"nodeType":"67","messageId":"68","endLine":1,"endColumn":26},{"ruleId":"6